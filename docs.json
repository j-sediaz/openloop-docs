{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Partners API Documentation",
  "colors": {
    "primary": "#e34376",
    "light": "#e55484",
    "dark": "#e3235f"
  },
  "favicon": "/favicon.ico",
  "api": {
    "playground": {
      "mode": "simple"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "pages": [
          "index",
          "quickstart",
          "best-practices"
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "api-reference/openapi.json",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "api-reference/introduction",
              "api-reference/data-standards"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "api-reference/programs-guide"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "OpenLoop Health",
          "href": "https://openloophealth.com",
          "icon": "globe"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/openloophealth",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/openloop_light.png",
    "dark": "/logo/openloop_dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "OpenLoop Health",
        "href": "https://openloophealth.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://openloophealth.com/contact"
    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://linkedin.com/company/openloophealth"
    }
  }
}